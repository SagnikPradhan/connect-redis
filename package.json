		"postinstall": "husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable"
	},
	"devDependencies": {
		"@swc/core": "^1.2.161",
		"@swc/helpers": "^0.3.8",
		"@types/node": "^17.0.23",
		"@types/tap": "^15.0.6",
		"husky": "^7.0.0",
		"ioredis": "^5.0.1",
		"pinst": "^3.0.0",
		"prettier": "^2.6.1",
		"redis": "^4.0.4",
		"rollup": "^2.70.1",
		"rollup-plugin-ts": "^2.0.5",
		"typescript": "^4.6.3"
	},
	"license": "MIT",
	"packageManager": "yarn@3.2.0"
