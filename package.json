{
	"name": "connect-redis",
	"description": "Redis session store for Connect",
	"version": "0.0.0",
	"browserslist": [
		"maintained node versions"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"build": "rollup -c",
		"watch": "rollup -cw",
		"prepublish": "yarn build",
